{
    "name": "web-ts-to-mock",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "prettier": "prettier . --write",
        "worker:test:dummy": "ts-node-esm  --project ./workers/dummyDataCreator/tsconfig.json  --experimental-specifier-resolution=node ./workers/dummyDataCreator/src/test.ts",
        "worker:build:dummy": "rollup --bundleConfigAsCjs -c workers/dummyDataCreator/rollup.config.js"
    },
    "dependencies": {
        "@monaco-editor/react": "^4.6.0",
        "lodash": "^4.17.21",
        "monaco-editor": "^0.45.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hot-toast": "^2.4.1",
        "react-icons": "^4.12.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "tslib": "^2.6.2",
        "vite-tsconfig-paths": "^4.2.3"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.3.1",
        "@koale/useworker": "^4.0.2",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "8.3.3",
        "@types/lodash": "^4.14.202",
        "@types/logger": "^0.0.5",
        "@types/node": "^20.10.6",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react-swc": "^3.3.2",
        "autoprefixer": "^10.4.16",
        "daisyui": "^4.5.0",
        "eslint": "^8.45.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "logger": "^0.0.1",
        "postcss": "^8.4.32",
        "prettier": "3.1.1",
        "rollup": "3.29.4",
        "rollup-plugin-esbuild": "5.0.0",
        "tailwindcss": "^3.4.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.0.2",
        "vite": "^4.4.5"
